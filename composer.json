{
  "name": "dimgraycat/split-testing",
  "description": "A server-side A/B/n testing tool",
  "keywords": ["optimizely", "ab", "split", "testing"],
  "homepage": "http://github.com/dimgraycat/php-split-testing",
  "license": "MIT",
  "authors":[
    {
      "name": "dimgraycat",
      "email": "dimgray.cat@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.0 || ^5.0 || ^6.0 || ^7.0 || ^8.0 || ^9.0",
    "squizlabs/php_codesniffer": "^2.9 || ^3.5"
  },
  "autoload": {
    "psr-4": {
      "Ab\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ab\\Tests\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}
